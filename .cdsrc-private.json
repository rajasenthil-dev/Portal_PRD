{
  "requires": {
    "[hybrid]": {
      "db": {
        "binding": {
          "type": "cf",
          "apiEndpoint": "https://api.cf.us20.hana.ondemand.com",
          "org": "McKesson Corporation_discovery-sunrise",
          "space": "Datasphere_Dev",
          "instance": "mck-portal-ds-db",
          "key": "mck-portal-ds-db-key",
          "resolved": false
        },
        "kind": "hana-cloud",
        "vcap": {
          "name": "db"
        }
      },
      "auth": {
        "binding": {
          "type": "cf",
          "apiEndpoint": "https://api.cf.us20.hana.ondemand.com",
          "org": "McKesson Corporation_discovery-sunrise",
          "space": "Datasphere_Dev",
          "instance": "mck-portal-auth",
          "key": "mck-portal-auth-key",
          "resolved": false,
          "credentials": {
            "xsappname": "mck-portal",
            "tenant-mode": "dedicated",
            "description": "Security profile of called application",
            "oauth2-configuration": {
              "grant-types": [
                "client_credentials",
                "authorization_code",
                "refresh_token",
                "urn:ietf:params:oauth:grant-type:jwt-bearer",
                "user_token"
              ],
              "credential-types": [
                "binding-secret",
                "x509"
              ],
              "system-attributes": [
                "groups"
              ],
              "allowedproviders": [],
              "redirect-uris": [
                "https://**cfapps.us20.hana.ondemand.com/**",
                "https://**cfapps.us20.hana.ondemand.com/my/logout",
                "https://*.applicationstudio.cloud.sap/**",
                "https://port5000-workspaces-ws-7hzlf.us20.applicationstudio.cloud.sap/"
              ]
            },
            "scopes": [
              {
                "name": "$XSAPPNAME.Viewer",
                "description": "Viewer scope with restricted access"
              }
            ],
            "attributes": [
              {
                "name": "SalesOrg",
                "description": "SalesOrg",
                "valueType": "string",
                "valueRequired": false
              },
              {
                "name": "ManufacturerNumber",
                "description": "ManufacturerNumber",
                "valueType": "string",
                "valueRequired": false
              },
              {
                "name": "SalesOffice",
                "description": "SalesOffice",
                "valueType": "string",
                "valueRequired": false
              },
              {
                "name": "customAttribute1",
                "description": "ProfitCentre",
                "valueType": "string",
                "valueRequired": false
              },
              {
                "name": "MFGName",
                "description": "type",
                "valueType": "string",
                "valueRequired": false
              }
            ],
            "role-templates": [
              {
                "name": "Viewer",
                "description": "Viewer Role with Restricted Access",
                "scope-references": [
                  "$XSAPPNAME.Viewer",
                  "uaa.user"
                ],
                "attribute-references": [
                  "family_name",
                  "given_name",
                  "SalesOrg",
                  "ManufacturerNumber",
                  "SalesOffice",
                  "customAttribute1",
                  "MFGName"
                ]
              }
            ],
            "role-collections": [
              {
                "name": "Viewer",
                "description": "Viewer User Role Collection",
                "role-template-references": [
                  "$XSAPPNAME.Viewer"
                ]
              }
            ]
          }
        },
        "kind": "xsuaa-auth",
        "vcap": {
          "name": "auth"
        }
      }
    }
  }
}