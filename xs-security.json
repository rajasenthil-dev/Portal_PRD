{
    "xsappname": "mck-portal",
    "tenant-mode": "dedicated",
    "description": "Security profile of called application",
    "scopes": [
        {
            "name": "uaa.user",
            "description": "UAA"
        },
        {
            "name": "$XSAPPNAME.mfg",
            "description": "scope from okta"
        }
    ],
    "role-templates": [
        {
            "name": "Token_Exchange",
            "description": "UAA",
            "scope-references": [
                "uaa.user"
            ]
        },
        {
            "name": "MFG",
            "description": "OKTA",
            "scope-references": [
                "$XSAPPNAME.mfg"
            ]
        }
    ],
    "oauth2-configuration": {
        "credential-types": [
            "binding-secret",
            "x509"
        ],
        "redirect-uris": [
            "https://**cfapps.us20.hana.ondemand.com/**"
        ]
    }
    
}