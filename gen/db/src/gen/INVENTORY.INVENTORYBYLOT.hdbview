VIEW INVENTORY_INVENTORYBYLOT AS SELECT
  ENTINVENTORYBYLOT_0.SKU,
  ENTINVENTORYBYLOT_0.PRODUCT_CODE,
  ENTINVENTORYBYLOT_0.PRODUCT_DESCRIPTION,
  ENTINVENTORYBYLOT_0.SIZE,
  ENTINVENTORYBYLOT_0.LOT,
  ENTINVENTORYBYLOT_0.EXPIRY_DATE,
  ENTINVENTORYBYLOT_0.QTY_ON_HAND,
  ENTINVENTORYBYLOT_0.DAYS_UNTIL_EXPIRY,
  ENTINVENTORYBYLOT_0.UPC,
  ENTINVENTORYBYLOT_0.CO,
  ENTINVENTORYBYLOT_0.MANUFACTURER,
  ENTINVENTORYBYLOT_0.WAREHOUSE
FROM INVENTORYBYLOT AS ENTINVENTORYBYLOT_0