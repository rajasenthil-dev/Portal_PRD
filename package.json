  "cds": {
    "service": {
      "path": "srv"
    },
    "localization": {
      "folders": [
        "app",
        "db",
        "webapp"
      ],
      "supportedLocales": [
        "en",
        "fr"
      ],
      "fallbackLocale": "en"
    },
    "hana": {
      "deploy-format": "hdbtable",
      "skip-generation": {
        "hdbview": true
      }
    },
    "requires": {
      "auth": {
        "kind": "xsuaa"
      },
      "logger": {
        "level": "debug"
      },
      "xsuaa_api": {
        "kind": "rest",
        "credentials": {
          "url": "https://discovery-sunrise.authentication.us20.hana.ondemand.com",
          "forwardAuthToken": true
        }
      },
      "approuter": {
        "kind": "cloudfoundry"
      },
      "db": {
        "kind": "sql",
        "log_sql": true
      },
      "html5-repo": true
    }
  },
  "sapux": [
    "app/shipstatus",
    "app/shipstatus1",
    "app/shipstatus"
  ],
  "workspaces": [
    "app/*"
  ]
