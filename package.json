    "watch-invsnapshot": "cds watch --open invsnapshot/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-invval": "cds watch --open invval/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-invoicehis": "cds watch --open invoicehis/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-openar": "cds watch --open openar/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-salesbycus": "cds watch --open salesbycus/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-bckorders": "cds watch --open bckorders/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project12345": "cds watch --open project12345/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-test1245": "cds watch --open test1245/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-backorders1": "cds watch --open backorders1/webapp/index.html?sap-ui-xx-viewCache=false",
