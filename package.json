  "scripts": {
    "start": "cds-serve",
    "test": "jest",
    "clean": "rimraf .portal_mta_build_tmp resources mta_archives Makefile_*.mta gen || true",
    "build": "npm run clean && npx mbt build --mtar archive",
    "build:cf": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/mck-portal_1.0.3.mtar --retries 1",
    "watch": "cds watch --profile hybrid",
    "watch-poc-app": "cds watch --open poc-app/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy mck-portal --delete-services --delete-service-keys --delete-service-brokers",
    "watch-project426": "cds watch --open project426/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-itemmas": "cds watch --open itemmas/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-dude": "cds watch --open dude/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-shiphis": "cds watch --open shiphis/index.html?sap-ui-xx-viewCache=false --livereload false",
