<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="shipstatus.ext.ListReportAfterHeader">

    <FlexBox id="_IDGenFlexBox1"
        width="100%"
        direction="Row"
        wrap="Wrap"
        justifyContent="SpaceBetween"
        class="headerPadding sapUiMediumMarginBottom borderBottom">

        <HeaderContainer id="_IDGenHeaderContainer" scrollStep="200" height="100px">
            <FlexBox id="_IDGenFlexBox2" alignItems="Center" height="100px">
                <FlexBox id="_IDGenFlexBox3" alignItems="Center" height="75px" class="imgBG">
                    <Image
                        id="logoImage"
                        src="/img/logoipsum-325.svg"
                        width="125px"
                        decorative="false"
                        press="onLogoPressed"
                        tooltip="SAP Logo" />
                </FlexBox>
            </FlexBox>
        </HeaderContainer>

    </FlexBox>

</mvc:View>

